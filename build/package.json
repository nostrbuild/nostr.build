{
  "name": "nostrbuild",
  "version": "1.0.0",
  "description": "JS Libraries for use in nostr.build app",
  "source": "src/index.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -r dist/",
    "watch": "parcel watch",
    "build": "parcel build src/*.js && npx tailwindcss -o ../styles/twbuild.css --minify && npm run update-versions",
    "postinstall": "node fix-imports.js",
    "update-versions": "/usr/bin/env python3 bump_css_js_version.py"
  },
  "author": "Fishcake",
  "license": "MIT",
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@uppy/url": "^3.6.0",
    "alpinejs": "^3.13.8",
    "nostr-tools": "^2.3.2",
    "sortablejs": "^1.15.2",
    "uppy": "^3.24.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "cssnano": "^6.1.2",
    "parcel": "2.12.0",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.3"
  },
  "targets": {
    "default": {
      "distDir": "../scripts/dist"
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
